<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.internet.myapplication.TelaCadastro"
    tools:showIn="@layout/activity_tela_cadastro">

    <android.support.design.widget.TextInputLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp">

    </android.support.design.widget.TextInputLayout>

    <android.support.design.widget.AppBarLayout>

        <LinearLayout
            android:layout_width="356dp"
            android:layout_height="300dp"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="14dp"
            tools:layout_editor_absoluteY="195dp">

        </LinearLayout>


    </android.support.design.widget.AppBarLayout>

    <include
        layout="@layout/content_main"
        android:layout_height="488dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"

    <TextView
        android:id="@+id/txtviewCPF"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="CPF"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="87dp" />

    <EditText
        android:id="@+id/edittxtCPF"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorTexto"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="87dp" />

    <TextView
        android:id="@+id/txtviewNome"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Nome"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="133dp" />

    <EditText
        android:id="@+id/edittxtNome"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorTexto"
        android:ems="10"
        android:inputType="textPassword"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="133dp" />

    <TextView
        android:id="@+id/txtviewConfirmarSenha"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Confirmar Senha"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="327dp" />

    <EditText
        android:id="@+id/edittxtConfirmarNovaSenha"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorTexto"
        android:ems="10"
        android:inputType="textPassword"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="327dp" />

    <Button
        android:id="@+id/button2"
        style="@style/Widget.AppCompat.Button.Borderless.Colored"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@color/colorButton"
        android:text="CONFIRMAR"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="132dp"
        tools:layout_editor_absoluteY="392dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Celular"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="179dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorTexto"
        android:ems="10"
        android:inputType="phone"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="179dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="E-mail"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="225dp" />

    <EditText
        android:id="@+id/editText5"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorTexto"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="225dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="Senha"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="271dp" />

    <EditText
        android:id="@+id/editText6"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:backgroundTint="@color/colorTexto"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="271dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="336dp"
        android:layout_height="wrap_content"
        android:text="NOVO CADASTRO"
        android:textColor="@color/colorTexto"
        tools:layout_editor_absoluteX="119dp"
        tools:layout_editor_absoluteY="31dp" />
</android.support.constraint.ConstraintLayout>